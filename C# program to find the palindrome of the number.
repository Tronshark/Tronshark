using System;

class t
{  
    static void Main(string[] args) 
    {  
        int num, rem, sum = 0, temp; 
        num = Convert.ToInt32(Console.ReadLine());  
        temp = num;  
        while (num > 0) 
        {  
            rem = num % 10;    
            num = num / 10;   
            sum = sum * 10 + rem;
        }  
        if (temp == sum)   
        {   
            Console.WriteLine("\n Number is Palindrome ");  
        } 
        else 
        {  
            Console.WriteLine("\n Number is not a palindrome ");  
        }  
        Console.ReadLine();  
    }  
}
